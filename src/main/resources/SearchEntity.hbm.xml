<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SearchEntity" table="search" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="keyword">
            <column name="keyword" sql-type="varchar(255)"/>
        </property>
        <property name="data">
            <column name="data" sql-type="text"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>