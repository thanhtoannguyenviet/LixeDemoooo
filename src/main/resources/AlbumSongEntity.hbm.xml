<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.AlbumSongEntity" table="Album_Song" schema="dbo" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="albumid">
            <column name="albumid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="songid">
            <column name="songid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>