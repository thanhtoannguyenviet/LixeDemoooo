<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.BannerEntity" table="banner" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="imagename">
            <column name="imagename" sql-type="text" not-null="true"/>
        </property>
        <property name="idlink">
            <column name="idlink" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>