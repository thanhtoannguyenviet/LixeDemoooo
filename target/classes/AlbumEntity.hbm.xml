<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.AlbumEntity" table="Album" schema="public" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="albumName">
            <column name="albumName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="listsongid">
            <column name="listsongid" sql-type="text" not-null="true"/>
        </property>
        <property name="datereleased">
            <column name="datereleased" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="index">
            <column name="index" sql-type="integer" not-null="true"/>
        </property>
        <property name="range">
            <column name="range" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>