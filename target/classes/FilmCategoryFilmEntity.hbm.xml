<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.FilmCategoryFilmEntity" table="Film_CategoryFilm" schema="public"
           catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="filmid">
            <column name="filmid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="categoryid">
            <column name="categoryid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>