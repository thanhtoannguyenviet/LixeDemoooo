<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SerifilmEntity" table="serifilm" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="listfilm">
            <column name="listfilm" sql-type="text"/>
        </property>
        <property name="ext">
            <column name="ext" sql-type="text"/>
        </property>
        <property name="index">
            <column name="index" sql-type="bigint"/>
        </property>
        <property name="range">
            <column name="range" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>