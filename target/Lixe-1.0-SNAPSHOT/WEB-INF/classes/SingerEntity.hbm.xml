<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SingerEntity" table="Singer" schema="public" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="singerName">
            <column name="singerName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="info">
            <column name="info" sql-type="text" not-null="true"/>
        </property>
        <property name="img">
            <column name="img" sql-type="text" not-null="true"/>
        </property>
        <property name="active">
            <column name="active" sql-type="bit" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>